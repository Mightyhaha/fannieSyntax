{
    "name": "fannie",
    "displayName": "Fannie Recipe Language",
    "description": "Syntax Support for Fannie Recipe Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fannie",
